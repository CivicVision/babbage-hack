---
title: Haushalt Rheinland-Pfalz
---
<div ng-controller="DemoCtrl">
  <babbage endpoint="http://52.23.213.247/api/babbage" cube="rheinland-pfalz" state="state">
    <div class='dimension-filters'>
      <p class="explanation">
      </p>
      <div class='filter'>
        <h4>Haushaltsjahr</h4>
        <div class='filter-element'>
          <babbage-cut-filter type='dropdown' filter="jahr" selected="2016" default-cut="defaultCut"></babbage-cut-filter>
        </div>
      </div>
      <div class='filter'>
        <h4>Anzeigen nach</h4>
        <div class='filter-element'>
          <dimension-filter filter='anzeige' selected="Plan" default-cut='defaultCut'>
            <div class="dropdown">
              <div class="btn-group" dropdown ng-show="filter.length">
                <a class="btn btn-default dropdown-toggle ng-click" dropdown-toggle>{{selected}} <span class="caret"></span></a>
                <ul class="dropdown-menu">
                  <li ng-repeat="attr in filter">
                    <a ng-click="update(attr)">
                      <strong>{{attr.label}}</strong>
                    </a>
                  </li>
                </ul>
              </div>
            </div>
          </dimension-filter>
        </div>
      </div>
      <div class='filter'>
        <h4>Typ</h4>
        <div class='filter-element'>
          <babbage-cut-filter filter="einahmenausgaben" selected="Ausgaben" default-cut="defaultCut">
            <div class="dropdown">
              <div class="btn-group" dropdown ng-show="filter.length">
                <a class="btn btn-default dropdown-toggle" dropdown-toggle class="ng-link">{{selected}} <span class="caret"></span></a>
                <ul class="dropdown-menu">
                  <li ng-repeat="attr in filter">
                    <a ng-click="update(attr)">
                      <strong>{{attr.label}}</strong>
                    </a>
                  </li>
                </ul>
              </div>
            </div>
          </babbage-cut-filter>
        </div>
      </div>
    </div>
    <treemap-breadcrumb reset='reset' state='state'></treemap-breadcrumb>
    <babbage-treemap show-tooltip=true tooltip-content="tooltipContent(d)"></babbage-treemap>
    <treemap-table></treemap-table>
  </babbage>
</div>
<div id="tooltip"></div>
